     "data": {
      "text/latex": [
       "$\\displaystyle ds^{2} = \\Omega r^{2} + dr^{2} \\left(\\frac{C}{r} + 1\\right) - dt^{2} \\left(\\frac{C}{r} + 1\\right)^{2}$"
      ],
      "text/plain": [
       "Eq(ds**2, Omega*r**2 + dr**2*(C/r + 1) - dt**2*(C/r + 1)**2)"
      ]
     },
     "execution_count": 28,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "eq1 = Eq(ds**2, -(1 + C/r)**2 * dt**2 + (1+C/r)**1 * dr**2 + r**2*omega)\n",
